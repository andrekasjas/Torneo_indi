@page
@model Torneo.App.Frontend.Pages.Jugadores.IndexModel
@{
}
<h1>Listado de Jugadores</h1>
<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Nombre</th>
            <th scope="col">Numero</th>
            <th scope="col">Equipo</th>
            <th scope="col">Posicion</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.jugadores)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Nombre</td>
                <td>@item.Numero</td>
                <td>@item.Equipo.Nombre</td>
                <td>@item.Posicion.Nombre</td>
            </tr>
        }
    </tbody>
</table>